        <class>org.wildfly.myzkapp.server.entity.pojo.CompanyHead</class>
        <class>org.wildfly.myzkapp.server.entity.pojo.CompanyBranch</class>
        <class>org.wildfly.myzkapp.server.entity.pojo.Address</class>
    </persistence-unit>

</persistence>
